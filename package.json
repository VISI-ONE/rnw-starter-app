{
	"name": "rnwStarterApp",
	"version": "0.0.1",
	"main": "index.js",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/VISI-ONE/rnw-starter-app"
	},
	"engines": {
		"node": "8.9.1"
	},
	"description": "React Native Web starter app with redux, react router, jest and enzyme",
	"author": {
		"name": "Or Yoffe",
		"url": "https://github.com/orYoffe"
	},
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "npm run test:web && npm run test:native",
		"test:web-watch": "node scripts/test.js --env=jsdom --watch",
		"test:web": "node scripts/test.js --env=jsdom",
		"test:native": "node scripts/test.js --n --config \"./config/jest/native.jest.config.js\"",
		"test:native-watch": "node scripts/test.js --n --watch --config \"./config/jest/native.jest.config.js\"",
		"coverage": "node scripts/test.js --env=jsdom --coverage",
		"coverage:native": "node scripts/test.js --n --config \"./config/jest/native.jest.config.js\" --coverage",
		"web": "node scripts/start.js",
		"build": "node scripts/build.js",
		"android": "node node_modules/react-native/local-cli/cli.js run-android",
		"ios": "node node_modules/react-native/local-cli/cli.js run-ios",
		"lint": "eslint \"src/**/*.js\" --color",
		"lint-fix": "eslint --fix \"src/**/*.js\" --color",
		"open-coverage": "open ./coverage/lcov-report/index.html",
		"open-coverage-native": "open ./coverageNative/lcov-report/index.html",
		"s": "npm run start",
		"t": "npm run test:web",
		"tw": "npm run test:web-watch",
		"tn": "npm run test:native",
		"tnw": "npm run test:native-watch",
		"w": "npm run web",
		"a": "npm run android",
		"i": "npm run ios",
		"c": "npm run coverage",
		"oc": "npm run open-coverage",
		"cn": "npm run coverage:native",
		"ocn": "npm run open-coverage-native",
		"precommit": "node scripts/precommit.js && npm run test && node scripts/postcommit.js",
		"prepush": "node scripts/precommit.js && npm run test && node scripts/postcommit.js",
		"postinstall": "node scripts/postinstall.js"
	},
	"dependencies": {
		"autoprefixer": "7.1.2",
		"babel-core": "6.25.0",
		"babel-eslint": "7.2.3",
		"babel-loader": "7.1.1",
		"babel-preset-react-app": "3.0.3",
		"babel-runtime": "6.26.0",
		"case-sensitive-paths-webpack-plugin": "2.1.1",
		"chalk": "1.1.3",
		"core-js": "^2.5.1",
		"css-loader": "0.28.4",
		"dotenv": "4.0.0",
		"eslint-config-react-app": "2.0.1",
		"eslint-loader": "1.9.0",
		"eslint-plugin-flowtype": "2.35.0",
		"extract-text-webpack-plugin": "3.0.0",
		"file-loader": "0.11.2",
		"fs-extra": "^4.0.2",
		"html-webpack-plugin": "2.29.0",
		"object-assign": "4.1.1",
		"postcss-flexbugs-fixes": "3.2.0",
		"postcss-loader": "2.0.6",
		"promise": "8.0.1",
		"prop-types": "15.6.0",
		"raf": "3.4.0",
		"react": "16.0.0",
		"react-dev-utils": "4.1.0",
		"react-dom": "16.0.0",
		"react-native": "0.50.3",
		"react-native-web": "0.1.14",
		"react-redux": "5.0.6",
		"react-router": "4.2.0",
		"react-router-dom": "4.2.2",
		"react-router-native": "4.2.0",
		"redux": "3.7.2",
		"redux-thunk": "2.2.0",
		"style-loader": "0.18.2",
		"sw-precache-webpack-plugin": "0.11.4",
		"url-loader": "0.5.9",
		"webpack": "3.5.1",
		"webpack-manifest-plugin": "1.2.1",
		"whatwg-fetch": "2.0.3"
	},
	"devDependencies": {
		"babel-jest": "21.2.0",
		"babel-plugin-transform-react-remove-prop-types": "0.4.10",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react-native": "4.0.0",
		"enzyme": "^3.2.0",
		"enzyme-adapter-react-16": "^1.1.0",
		"enzyme-to-json": "^3.2.2",
		"eslint": "4.11.0",
		"eslint-config-airbnb": "16.1.0",
		"eslint-plugin-import": "2.8.0",
		"eslint-plugin-jsx-a11y": "6.0.2",
		"eslint-plugin-react": "7.5.1",
		"glob": "^7.1.2",
		"husky": "0.14.3",
		"jest": "21.2.1",
		"jest-fetch-mock": "^1.3.3",
		"lint-staged": "5.0.0",
		"react-test-renderer": "16.0.0",
		"webpack-dev-server": "2.8.2"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			"<rootDir>/src/index",
			"<rootDir>/src/utils/routing/",
			"<rootDir>/src/components/Picker/NativePicker.js"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.js?(x)",
			"<rootDir>/src/**/?(*.)(spec|test).js?(x)"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer",
			"react-native-web/jest/serializer"
		]
	},
	"babel": {
		"presets": [
			"react-native"
		],
		"env": {
			"test": {
				"plugins": [
					"react-native-web/babel"
				]
			},
			"development-web": {
				"plugins": [
					"react-native-web/babel"
				]
			},
			"production-web": {
				"plugins": [
					"react-native-web/babel"
				]
			},
			"test:native": {},
			"development": {},
			"production": {}
		}
	},
	"lint-staged": {
		"src/**/*.js": [
			"lint-fix",
			"git add"
		]
	}
}
